{
    "name": "kitchensink.api",
    "version": "0.0.1",
    "description": "API for the Kitchen Sink clients",
    "main": "dist/serverBootstrap.js",
    "scripts": {
        "clean": "rimraf dist/* dist/.env",
        "copy-assets": "ts-node tools/copyAssets",
        "lint": "eslint src --ext .ts",
        "tsc": "tsc",
        "build": "npm-run-all clean lint tsc",
        "dev:start": "npm-run-all build start",
        "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
        "start": "node .",
        "test": "echo \"Error: no test specified\" && exit 1",
        "upload-file": "node ./publish.js",
        "install-deploy-packages": "npm --prefix ./dist install ./dist",
        "deploy": "npm-run-all clean lint tsc copy-assets upload-file"
    },
    "dependencies": {
        "@automapper/classes": "^7.0.2",
        "@automapper/core": "^7.0.2",
        "@automapper/pojos": "^7.0.2",
        "@automapper/sequelize": "^7.0.2",
        "body-parser": "1.19.0",
        "cors": "2.8.5",
        "dotenv": "10.0.0",
        "express": "4.17.1",
        "express-winston": "4.2.0",
        "inversify": "6.0.1",
        "inversify-express-utils": "6.4.3",
        "mariadb": "2.5.5",
        "moment": "2.29.1",
        "moment-timezone": "0.5.33",
        "reflect-metadata": "0.1.13",
        "request": "2.88.2",
        "sequelize": "6.9.0",
        "sequelize-adapter": "2.1.5",
        "sequelize-pool": "7.1.0",
        "sequelize-transparent-cache": "2.3.0",
        "sequelize-transparent-cache-memcached": "2.2.3",
        "sequelize-typescript": "2.1.1",
        "serverless-sequelize-migrations": "1.1.1",
        "strong-error-handler": "4.0.0",
        "winston": "3.3.3"
    },
    "devDependencies": {
        "@types/bluebird": "3.5.36",
        "@types/cors": "2.8.12",
        "@types/dotenv": "8.2.0",
        "@types/express": "4.17.13",
        "@types/fs-extra": "9.0.13",
        "@types/moment-timezone": "0.5.30",
        "@types/node": "13.7.4",
        "@types/request": "2.48.7",
        "@types/shelljs": "0.8.9",
        "@types/validator": "13.6.6",
        "@typescript-eslint/eslint-plugin": "5.3.1",
        "@typescript-eslint/parser": "5.3.1",
        "archiver": "5.3.0",
        "eslint": "7.32.0",
        "eslint-plugin-jsdoc": "^37.0.3",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "fs-extra": "10.0.0",
        "nodemon": "2.0.14",
        "npm-run-all": "4.1.5",
        "rimraf": "3.0.2",
        "shelljs": "0.8.4",
        "sqlite3": "5.0.2",
        "ts-node": "10.4.0",
        "typescript": "4.4.4"
    }
}
